<Window x:Class="DifPlaksin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DifPlaksin"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="51*"/>
            <RowDefinition Height="310*"/>
            <RowDefinition Height="59*"/>
        </Grid.RowDefinitions>
        <Rectangle Fill="Aquamarine"></Rectangle>
        <TextBlock HorizontalAlignment="Center" FontSize="30" x:Name="TitlePage"></TextBlock>
        <Button x:Name="BackBtn" Margin="0 16 100 16" HorizontalAlignment="Right" Style="{StaticResource Button}" Click="BackBtn_Click">Назад</Button>
        <Button x:Name="ExitBtn" Margin="0 16 10 16" HorizontalAlignment="Right" Style="{StaticResource Button}" Click="ExitBtn_Click">Выход</Button>
        <Frame Grid.Row="1" Grid.RowSpan="2" x:Name="MyFrame" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
